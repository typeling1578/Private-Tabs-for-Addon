{
    "manifest_version": 2,
    "name": "Private Tabs",
    "version": "1.3.2",
    "default_locale": "en",
    "applications": {
        "gecko": {
            "id": "private-tabs@typeling1578",
            "strict_min_version": "115.0"
        }
    },
    "icons": {
        "48": "icons/icon-48x48.png",
        "96": "icons/icon-96x96.png"
    },
    "permissions": [
        "contextualIdentities",
        "cookies",
        "menus",
        "storage",
        "tabs",
        "webNavigation",
        "history"
    ],
    "browser_action": {
        "default_icon": {
            "48": "icons/icon-newtab-48x48.png",
            "96": "icons/icon-newtab-96x96.png"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
